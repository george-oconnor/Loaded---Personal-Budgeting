{
  "cli": {
    "version": ">= 5.9.1",
    "appVersionSource": "local"
  },
  "build": {
    "preview": {
      "ios": {
        "simulator": true
      }
    },
    "preview2": {
      "android": {
        "gradleCommand": ":app:assembleRelease"
      }
    },
    "preview3": {
      "developmentClient": true
    },
    "production": {
      "ios": {
        "resourceClass": "medium"
      }
    },
    "testflight": {
      "ios": {
        "resourceClass": "medium"
      },
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundleRelease"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6756985580",
        "groups": ["External Testers"]
      }
    },
    "testflight": {
      "ios": {
        "ascAppId": "6756985580",
        "groups": ["External Testers"]
      }
    }
  }
}
